{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"compilerOptions": {
    "jsx": "react-native",
		"baseUrl": "./",
		"paths": {
			"@app/*": ["src/app/*"],
			"@assets/*": ["src/assets/*"],
			"@atomic/*": ["src/atomic/*"],
		},
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	}
}
